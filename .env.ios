# iOS Development Environment Variables
# These are used when running iOS development builds

# Development server host for iOS device access
TAURI_DEV_HOST=0.0.0.0

# iOS deployment target
IPHONEOS_DEPLOYMENT_TARGET=14.0

# iOS Build Optimization
RUST_BACKTRACE=0
CARGO_PROFILE_RELEASE_OPT_LEVEL=3
CARGO_PROFILE_RELEASE_LTO=true

# Disable debug symbols in release builds for smaller app size
CARGO_PROFILE_RELEASE_DEBUG=false

# Enable iOS-specific optimizations
CARGO_PROFILE_RELEASE_IOS_OPT_LEVEL=s
CARGO_PROFILE_RELEASE_IOS_LTO=true
CARGO_PROFILE_RELEASE_IOS_CODEGEN_UNITS=1
CARGO_PROFILE_RELEASE_IOS_PANIC=abort
CARGO_PROFILE_RELEASE_IOS_STRIP=true